    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('css/vendors_front.css' | asset_version ) }}"/>


        <!-- Font Awesome CSS -->
        <link href="fonts/font-awesome/css/font-awesome.css" rel="stylesheet">

        <!-- Fontello CSS -->
        <link href="fonts/fontello/css/fontello.css" rel="stylesheet">

        <link rel="stylesheet" href="{{ asset('/css/plugins_front.css' | asset_version ) }}"/>
        <link rel="stylesheet" href="{{ asset('css/main_front.css' | asset_version ) }}"/>
    {% endblock %}
    {% block javascripts_head %}
    {% endblock %}
</head>

<!-- body classes:  -->
<!-- "boxed": boxed layout mode e.g. <body class="boxed"> -->
<!-- "pattern-1 ... pattern-9": background patterns for boxed layout mode e.g. <body class="boxed pattern-1"> -->
<!-- "transparent-header": makes the header transparent and pulls the banner to top -->
<body class="no-trans  ">

<!-- scrollToTop -->
<!-- ================ -->
<div class="scrollToTop circle"><i class="icon-up-open-big"></i></div>

<!-- page wrapper start -->
<!-- ================ -->
<div class="page-wrapper">

    {% block header %}
    {% use "header.twig"  %}
    <!-- header-container start -->
    <div class="header-container">
        {{ block('header_top') }}
        {{ block('header_main') }}
    </div>
    <!-- header-container end -->
    {% endblock %}

</div>
<!-- page-wrapper end -->

<div id="page-start"></div>

{% block content %}{% endblock %}


{% block javascripts %}
    <!-- Javascripts
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>-->
    <script>window.jQuery || document.write('<script src="{{ asset('js/jquery.js' | asset_version ) }}"><\/script>')</script>
    <script src="{{ asset('js/vendors_front.js' | asset_version ) }}"></script>

    <script src="{{ asset('js/plugins.js' | asset_version ) }}"></script>

    <script src="{{ asset('js/custom.js' | asset_version ) }}"></script>
{% endblock %}
</body>
</html>